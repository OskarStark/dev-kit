{% extends 'base.html.twig' %}

{% block body %}
    <h1>Sonata Dev Kit</h1>

    <p>
        Revision:
        {% if revision %}
            <a href="https://github.com/sonata-project/dev-kit/commit/{{ revision }}">
                {{ revision }}
            </a>
        {% else %}
            N/A
        {% endif %}
    </p>

    <p>
        <a href="https://github.com/pulls?q=is%3Aopen+is%3Apr+author%3ASonataCI+archived%3Afalse">
            Open DevKit PR's
        </a>
    </p>
{% endblock %}
